<?xml version="1.0"?>
<AmazonMWS>
    <acct_accessKey>xxxxx</acct_accessKey>
    <acct_secretKey>xxxxx</acct_secretKey>
    <acct_appName>mumapp</acct_appName>
    <acct_appVersion>V0.10</acct_appVersion>
    <acct_serviceURL>https://mws.amazonservices.com</acct_serviceURL>
    <acct_sellerId>xxxx</acct_sellerId>
    <acct_mwsAuthToken>xxxxx</acct_mwsAuthToken>
    <acct_marketplaceId>xxxxx</acct_marketplaceId>
    <RequestASINList>
        <ASIN>B00L21BZ14</ASIN>
        <ASIN>B004TI336W</ASIN>
        <ASIN>B00EXBDWZC</ASIN>
    </RequestASINList>
    <HistoryPriceFile>E:/workspace/java/AmazonMWS3/dist/history_price.txt</HistoryPriceFile>
    <LogFile>E:/workspace/java/AmazonMWS3/dist/get_price.log</LogFile>
    <Strategy common="">
        <ASIN>B00L21BZ14</ASIN>
        <PriceRangeMin common="商品价格，不包含运费">10.15</PriceRangeMin>
        <PriceRangeMax common="商品价格，不包含运费">10.45</PriceRangeMax>
        <SetPriceDiff common="设置与Cmp价格的差异，+表示比它高，-表示比它低，0.0表示相同">-0.01</SetPriceDiff>
        <Cmp_Type select="competitive|Lowest">competitive</Cmp_Type>
        <Cmp_Price select="landed|listing">listing</Cmp_Price>
        <Condition_MyPriceHigh>0.01</Condition_MyPriceHigh>
        <Condition_MyPriceLow>0.01</Condition_MyPriceLow>
    </Strategy>
</AmazonMWS>